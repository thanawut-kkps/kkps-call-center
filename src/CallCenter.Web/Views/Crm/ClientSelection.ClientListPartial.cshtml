@model List<CallCenterGetClientListFromPhonenoResult>
<div style="margin-top: 10px;">
    <table class="table table-striped @(Model.Count > 0 ? "sortable" : "")">
        <thead>
            <tr>
                <th class="text-center">
                    PID
                </th>
                <th class="">
                    PID Type
                </th>
                <th class="">
                    Client Name Thai
                </th>
                <th class="">
                    Client Name Eng
                </th>
                <th class="">
                    Birth Date
                </th>
                <th class="">
                    Tel Home
                </th>
                <th class="">
                    Tel Office
                </th>
                <th class="">
                    Mobile
                </th>
                <th class="">
                    Address Mail
                </th>
                <th class="">
                    Contact Name Thai
                </th>
                <th class="">
                    Contact Name Eng
                </th>
                <th class="">
                    Contact Relationship
                </th>
                <th class="">
                    Contact Tel Home
                </th>
                <th class="">
                    Contact Tel Office
                </th>
                <th class="">
                    Contact Mobile
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                <tr>
                    <td class="text-center">
                    <a onclick="onClientSelected(@(item.ClientId ?? -1))" href="javascript:void(0);" class="account-no-link">@item.PidNo</a>
                      
                    </td>
                    <td class="">
                       @item.PidTypeDesc
                    </td>
                    <td class="">
                        @item.ClientNameT
                    </td>
                    <td class="">
                        @item.ClientNameE
                    </td>
                    <td class="">
                        @item.BirthDate
                    </td>
                    <td class="">
                        @item.TelHome
                    </td>
                    <td class="">
                       @item.TelOffice
                    </td>
                    <td class="">
                       @item.Mobile
                    </td>
                    <td class="">
                        @item.AddrMail
                    </td>
                    <td class="">
                        @item.ContactNameT
                    </td>
                    <td class="">
                        @item.ContactNameE
                    </td>
                    <td class="">
                        @item.ContactRelationship
                    </td>
                    <td class="">
                        @item.ContactTelHome
                    </td>
                    <td class="">
                        @item.ContactTelOffice
                    </td>
                    <td class="">
                        @item.ContactMobile
                    </td>
                </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="15" class="text-center">
                        There is no data.
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
