@model List<CallCenterGetDrilldownLoginResult>
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" id="form-dialog-label">
        Account Mapping with this client login
    </h4>
</div>
<div class="modal-body">
    <table class="table table-striped @(Model.Count > 0 ? "sortable" : "")">
        <thead>
            <tr>
                <th class="col-md-3">
                    Name Eng
                </th>
                <th class="col-md-3">
                    Name Thai
                </th>
                <th class="col-md-2 text-center">
                    Account No
                </th>
                <th class="col-md-2 text-center" >
                    Front Type
                </th>
               <th class="col-md-2 text-center" >
                    Can Trade
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                <tr>
                    <td>
                        @item.ClientNameE
                    </td>
                    <td>
                        @item.ClientNameT
                    </td>
                    <td class="text-center">
                       @item.AccountNo
                    </td>
                   <td class="text-center">
                       @item.FrontTypeDesc
                    </td>
                   <td class="text-center">
                       @item.CanTrade
                    </td>
                </tr> 
                }
            }
            else
            {
                <tr>
                    <td colspan="5" class="text-center row-no-data">
                        There is no data.
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">
        Close</button>
</div>
