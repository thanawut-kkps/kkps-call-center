@model List<CallCenterGetAccountMutualFundResult>
<div style="margin-top: 10px;" id="mutualFundInfoContainer">
    <table class="table table-striped @(Model.Count > 0 ? "sortable" : "")">
        <thead>
            <tr>
                <th class="text-center can-hide-label">
                    Account No.
                </th>
                <th class="text-center can-hide-label">
                    AMC
                </th>
                <th class="text-center can-hide-label">
                    Holder Id
                </th>
                <th class="text-center can-hide-label">
                    Name
                </th>
                <th class="text-center can-hide-label">
                    Status
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count > 0)
            {                
                foreach (var item in Model)
                {
                    <tr>
                        <td class="text-center can-hide-value">
                            @item.AccountNo
                        </td>
                        <td class="text-center can-hide-value">
                            @item.AmcCode
                        </td>
                        <td class="text-center can-hide-value">
                            @item.HolderId
                        </td>
                        <td class="text-center can-hide-value">
                            @item.HolderNameT
                        </td>
                        <td class="text-center can-hide-value">
                            @item.StatusDesc
                        </td>
                    </tr> 
                }
            }
            else
            { 
             <tr class="row-no-data">
                <td colspan="5" class="text-center can-hide-value">No data.</td>
             </tr>            
            }
        </tbody>
    </table>
</div>
