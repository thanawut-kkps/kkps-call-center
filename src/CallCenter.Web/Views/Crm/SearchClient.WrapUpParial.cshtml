@model WrapUpViewModel
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" id="form-dialog-label">
        Wrap Up</h4>
</div>
<div class="modal-body">
    @Html.Action("IndexWrapUpPartial", new {  _agentId = Model.AgentId,
                                              _callSession = Model.CallSession,
                                              _callStartTime = Model.CallStartTime,
                                              _customerPhone = Model.CustomerPhone,
                                              _extension = Model.Extension
                                          }) 
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">
        Close</button>
</div>
