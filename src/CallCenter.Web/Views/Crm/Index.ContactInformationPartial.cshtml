@model CallCenter.Web.ViewModels.ClientInfoViewModel
<div class="row nomargin sub-title space-above">
    <div class="col-md-12 nopadding">
        <i class="fa fa-user"></i>&nbsp;Contact Information
    </div>
</div>
<div class="row nomargin row-detail">
    <div class="col-md-3 nopadding">
        <strong>Address1 Register</strong>
    </div>
    <div class="col-md-9 nopadding verify-addr-register">
        @Model.ProfileResult.AddrRegister &nbsp;
    </div>
</div>
<div class="row nomargin row-detail">
    <div class="col-md-3 nopadding">
        <strong>Address2 Mailing</strong>
    </div>
    <div class="col-md-9 nopadding verify-addr-mail">
        @Model.ProfileResult.AddrMail &nbsp;
    </div>
</div>
<div class="row nomargin">
    <div class="col-md-12 nopadding">
        @Html.Partial("Index.ContactAddressPartial", Model.AddressResults)
    </div>
</div>
